{
    "name": "lmlynski/schedule-api-remastered",
    "description": "Simple over engineered schedule REST API",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/dbal": "^4.0",
        "doctrine/doctrine-bundle": "^2.12",
        "ramsey/uuid": "^4.7",
        "symfony/console": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^6.3",
        "symfony/monolog-bundle": "^3.10",
        "symfony/runtime": "^6.3",
        "symfony/validator": "^6.3",
        "symfony/yaml": "^6.3",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.58",
        "harmbandstra/swagger-ui-bundle": "^5.0",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/phpunit-bridge": "^7.1"
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ScheduleApiRemastered\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScheduleApiRemastered\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "HarmBandstra\\SwaggerUiBundle\\Composer\\ScriptHandler::linkAssets",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "HarmBandstra\\SwaggerUiBundle\\Composer\\ScriptHandler::linkAssets",
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    }
}
